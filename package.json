  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.11.5",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "glob": "^7.1.6",
    "js-beautify": "^1.12.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0"
